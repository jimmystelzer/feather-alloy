{
  "productName": "Feather Alloy",
  "version": "0.1.0",
  "identifier": "com.feather.alloy",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://localhost:1420",
    "frontendDist": "ui"
  },
  "app": {
    "windows": [
      {
        "title": "Feather Alloy",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
